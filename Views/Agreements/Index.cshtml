@model IEnumerable<AgreementsModel>
@{ Layout = "_Layout"; ViewBag.Title = "Agreements List";}

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.Id</td>
                <td>@r.Name</td>
                <td>@r.Description</td>
                <td>@r.Amount</td>
            </tr>
        }
    </tbody>
</table>